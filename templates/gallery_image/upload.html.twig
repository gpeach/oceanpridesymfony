{% extends 'base.html.twig' %}

{% block title %}Upload Image{% endblock %}

{% block body %}
    <h1>Upload an Image</h1>
    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.file) }}
        <button type="submit">Upload</button>
    {{ form_end(form) }}
{% endblock %}
